    "pi:inscription1": [
        {
            "@value": "Sedia curule di marmo in prospettiva Essa conservasi nel Museo del Cavalier Piranesi, ritrovata al tempo di PP. Paolo III. nel Foro Romano nel luogo anticamente detto Rostri; oggidi vicino alla Chiesa di S. Teodoro. A Suppedaneo, architettato con arabeschi ed altri ornamenti.",
            "is_public": true,
            "property_id": 427,
            "property_label": "inscription1",
            "type": "literal"
        }
    ],
    "pi:inscription1_tr": [
        {
            "@value": "大理石製高官椅子を透視図法で描いたもの。騎士ピラネージの陳列室に保管されている。教皇パウルス3世〔在位,1534 - 1549年〕の時代に,古代にはロストラ〔フォルム・ロマヌムに設置されていた船嘴で装飾された演壇〕と呼ばれていた場所で発見された。この場所は,今日のサン・テオドーロ教会の近くである。A. アラベスク文様やその他の装飾で構成されている足台。",
            "is_public": true,
            "property_id": 428,
            "property_label": "inscription1_tr",
            "type": "literal"
        }
    ],
    "pi:inscription2": [
        {
            "@value": "Altra veduta deretana della sedia medesimaVaso antico di marmo, oggidi presso il Sig.r Guglielmo Smyth in Londra.",
            "is_public": true,
            "property_id": 429,
            "property_label": "inscription2",
            "type": "literal"
        }
    ],
    "pi:inscription2_tr": [
        {
            "@value": "同じ椅子の背面図古代の大理石製の壷。今日はロンドンのウィリアム・スミス氏のもとにある。",
            "is_public": true,
            "property_id": 430,
            "property_label": "inscription2_tr",
            "type": "literal"
        }
    ],
