    "pi:hf_desc": [
        {
            "@value": "302. XVII. Circonferenza della pianta terrena del mausoleo di Sant' Elena... -- Le mot terrena est ajouté au-dessus de la ligne. -- Sig. en bas à dr. -- H. 0.36. L. 0.227. -- Fil. id.",
            "is_public": true,
            "property_id": 416,
            "property_label": "HF_desc",
