    "pi:hf_desc": [
        {
            "@value": "259. XXXVII. Spaccato del sepolcro antecedente. -- Une réserve pour l'échelle, placée en écriteau, à cheval sur la gravure et sur le titre. -- Sig. en bas à dr. -- H. 0.36. L. 0.52. --Fil. id.",
            "is_public": true,
            "property_id": 416,
            "property_label": "HF_desc",
