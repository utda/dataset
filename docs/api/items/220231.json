    "pi:inscription1": [
        {
            "@value": "Fig. I. A Lapides Albani. B Reliquiae operis reticulati operi Lapideo interjecti, ut in Fig. II. C Emplecton operis reticulati. D Mons e topho. Fig. III. E Sectio Montis Capitolini sive rupis Tarpejae. F Sectio Lapidum Albanorum.",
            "is_public": true,
            "property_id": 427,
            "property_label": "inscription1",
            "type": "literal"
        }
    ],
    "pi:inscription1_tr": [
        {
            "@value": "図I. A ペペリーノ[火山砕屑岩]。B 図II.に示されるように、岩の部分の中に挿入された編目積みの部分。C 編目積みの部分の中のエンプレクトン積み[粗石積み]。D 凝灰岩からなる丘。図III. Eカピトリヌス丘、タルペイア1の断崖の断面図。F ペペリーノの断面図。",
            "is_public": true,
            "property_id": 428,
            "property_label": "inscription1_tr",
            "type": "literal"
        }
    ],
    "pi:inscription2": [
        {
            "@value": "Reliquae Substructionum Capitolij e Saxo quadrato, quae factae sunt anno CCCLXVI. Visuntur in cavaedio Xenodochii S. Mariae Consolationis ad Lavacrum Linteorum.Fig. II. Schema Substructionum Capitolij, prout erant antiquitus, ex reliquijs desumptum.",
            "is_public": true,
            "property_id": 429,
            "property_label": "inscription2",
            "type": "literal"
        }
    ],
    "pi:inscription2_tr": [
        {
            "@value": "切石で作られたカピトリヌス丘の基部の遺構,366年に作られたもの。洗濯場に面する、サンタ・マリア・デッラ・コンソラツィオーネ教会に附随する巡礼宿の中庭に見られる。図II. カピトリヌス丘の基部の図。遺構から復元した古代の様子。",
            "is_public": true,
            "property_id": 430,
            "property_label": "inscription2_tr",
            "type": "literal"
        }
    ],
