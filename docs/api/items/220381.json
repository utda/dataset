    "pi:inscription1_tr": [
        {
            "@value": "I〔ジョヴァンニ〕. B〔バッティスタ〕. ピラネージ作,カピトリヌス丘の石碑,あるいはローマ建国からティベリウス帝までの執政官表および凱旋将軍表,ローマ建国からティベリウス帝まで",
            "is_public": true,
            "property_id": 428,
            "property_label": "inscription1_tr",
            "type": "literal"
        }
    ],
    "pi:inscription2": [
        {
            "@value": "L BRUTVS M(ARCI) F(LIVS)CAESAR IMP(ERATOR)CN POMP(EIVS) MAG(NVS) IMP(ERATOR)AETERNA VRBS ROMACAESAR IIIVIR R[EI] P(VBLICAE) C(ONSTITVENDAE)M ANTONIVS IIIVIR R(EI)P(VBLICAE) C(ONSTITVENDAE)(O)PVS DOLIARE EX PRA(EDIIS)C MARC(IO) C ASINIO CO(N)S(ULIBVS)M LEPIDVS IIIVIR R(EI) P(VBLICAE) C(ONSTITVENDAE)CÆSAR DIC(TATOR) PER(PETVO)S(ENATVS) C(ONSVLTO)",
            "is_public": true,
            "property_id": 429,
            "property_label": "inscription2",
            "type": "literal"
        }
    ],
    "pi:inscription2_tr": [
        {
            "@value": "マルクス・ブルトゥスの息子ルキウス・ブルトゥス最高司令官カエサル最高司令官グナエウス・ポンペイウス・マグヌス永遠の都市ローマ国家再建三人委員カエサル〔・オクタウィアヌス〕国家再建三人委員マルクス・アントニウス〔原文欠損〕の地所から作られた陶器執政官ガイウス・マルキウスとガイウス・アシニウス〔の年〕1国家再建三人委員マルクス・レピドゥス終身独裁官カエサル元老院の決議により",
            "is_public": true,
            "property_id": 430,
            "property_label": "inscription2_tr",
            "type": "literal"
        }
