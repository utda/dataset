    "pi:hf_desc": [
        {
            "@value": "441. XI. Scenographia Insulae Tiberinae. -- Le titre dans un cartouche occupant toute la moitié gauche. -- Sig. à dr. en bas, dans le cartouche. -- H. 0.205. L. 0.44. -- Fil. id.",
            "is_public": true,
            "property_id": 416,
            "property_label": "HF_desc",
            "type": "literal"
        }
    ],
    "pi:hf_desc_tr": [
        {
            "@value": "441。図版11。ティベリーナ島の景観図。−左〔下部〕半分をすっかり占めているカルトゥーシュにタイトル。−カルトゥーシュの中,右下に署名。−高さ0.205,長さ0.44。−地図の周囲に枠線。",
            "is_public": true,
            "property_id": 417,
            "property_label": "HF_desc_tr",
