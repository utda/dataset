    "pi:we_desc": [
        {
            "@value": "After Sir Roger Newdigate, of Arbury Hall, Warwickshire, when in Rome during his second grand tour in 1775, presented Piranesi at his request with a drawing of the arch, made by him the previous September, from which the artist immediately made this plate(see McCarthy, op.cit.).Signature on a scroll: Disegnato dal cav. Rug. ro Newdigate Inglese. Cav.Piranesi incis.",
            "is_public": true,
            "property_id": 463,
            "property_label": "WE_desc",
            "type": "literal"
        }
    ],
