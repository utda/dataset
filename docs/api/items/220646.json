    "pi:inscription1": [
        {
            "@value": "Veduta in prospettiva dell'altro Candelabro antico che si vede nel Museo del Cavalier Piranesi",
            "is_public": true,
            "property_id": 427,
            "property_label": "inscription1",
            "type": "literal"
        }
    ],
    "pi:inscription1_tr": [
        {
            "@value": "騎士ピラネージの陳列館にある別の古代の燭台の透視図",
            "is_public": true,
            "property_id": 428,
            "property_label": "inscription1_tr",
            "type": "literal"
        }
    ],
    "pi:inscription2": [
        {
            "@value": "Al Signor Carlo Morris Cavaliere IngleseIl presente Candelabro o Signore, e stato rinvenuto nella Villa Adriana presso Tivoli nel luogo detto Pantanello. L'eleganza, e finezza dè suoi intagli, e le parti, che il tutto compongono, sono di una proporzione mirabile.In segno d'Ossequio Il Cavaliere Gio. Batt(ist)a Piranesi",
            "is_public": true,
            "property_id": 429,
            "property_label": "inscription2",
            "type": "literal"
        }
    ],
    "pi:inscription2_tr": [
        {
            "@value": "英国騎士チャールズ・モリス1様へ閣下,この燭台はティヴォリのハドリアヌス帝の別荘におけるパンタネッロ[小さな湿地,沼地]と呼ばれる場所にて発見されました。その彫刻装飾の優雅さ,繊細さは,素晴らしいもので,全体を構成する各部は驚くほどに調和しています。騎士ジョヴァンニ・バッティスタ・ピラネージより,敬意の印に",
            "is_public": true,
            "property_id": 430,
            "property_label": "inscription2_tr",
            "type": "literal"
        }
    ],
