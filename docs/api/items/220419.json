    "pi:hf_desc": [
        {
            "@value": "429. Deuxième frontispice. -- Une vue du Champ de Mars à vol d'oiseau. Le titre italien sur une table de pierre entourée d'une bordure ornementale et accolée de deux fig. de femmes, mutilées. -- Sig. en bas à gauche. -- H. 0.445. L. 0.235. -- Fil. id. (Ces deux frontispices sont parfois intervertis).",
            "is_public": true,
            "property_id": 416,
            "property_label": "HF_desc",
            "type": "literal"
        }
    ],
    "pi:hf_desc_tr": [
        {
            "@value": "429。2番目の扉絵。−カンプス・マルティウスの鳥瞰図。石版にイタリア語のタイトル。石版は装飾的な縁どりで囲まれ,両脇に手足のない2体の女性像がある。−左下に署名。−高さ0.445,長さ0.235。−二重の枠線。 (これら2枚の扉絵の順序は時折〔版によって〕逆になっている) 。",
            "is_public": true,
            "property_id": 417,
            "property_label": "HF_desc_tr",
            "type": "literal"
        }
