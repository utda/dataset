    "pi:inscription1": [
        {
            "@value": "Labrum aegyptiacum porphyreticum, repertum inter rudera thermarum M. Agrippae, nunc urna Sepulchralis Cinerum S. M. Clementis XII. in basilica S. Ioannis Lateranensis. Operculum haud efformatum est, quod recens factum, cum Labrum pro urna usurpatum est.",
            "is_public": true,
            "property_id": 427,
            "property_label": "inscription1",
            "type": "literal"
        }
    ],
    "pi:inscription1_tr": [
        {
            "@value": "アグリッパ浴場〔実際は、おそらくネプトゥヌスのバシリカ〕の遺構で発見されたのエジプト産斑岩製の水盤。現在は,サン・ジョヴァンニ・イン・ラテラーノ聖堂にある教皇クレメンス12世〔在1730年-1740年〕の石棺となっている。蓋は当初は制作されておらず,近年になって,水盤を石棺として利用する際に造られたものである。",
            "is_public": true,
            "property_id": 428,
            "property_label": "inscription1_tr",
            "type": "literal"
        }
    ],
