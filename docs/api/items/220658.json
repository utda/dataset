    "pi:hf_desc": [
        {
            "@value": "Bas-relief en terre cuite représentant un quadrige dans une course (cabinet de l'auteur, puis en Angleterre). -- En bas, notes. -- Sig. Francesco Piranesi, 1791, en bas à dr., sous le fil. -- H. 0.38. L. 0.52. -- Double fil. d'encad.",
            "is_public": true,
            "property_id": 416,
            "property_label": "HF_desc",
